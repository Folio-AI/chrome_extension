{
  "name": "folio-resume-tailorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-watch",
    "start": "next start",
    "lint": "next lint",
    "build": "npm run build:content && rm -rf out && next build && next export && npm run lint:build",
    "lint:build": "mv out/_next out/assets && gsed -i 's/\\/_next/\\/assets/g' out/**.html",
    "transpile:inject": "webpack --mode production",
    "build:content": "webpack --config webpack.config.js --mode production"
  },
  "dependencies": {
    "next": "13.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-watch": "^0.11.0",
    "postcss": "^8",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.6",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "watch": {
    "build": {
      "patterns": [
        ".",
        "styles/**",
        "pages/**",
        "public/**"
      ],
      "ignore": [
        "out",
        "node_modules",
        ".next"
      ],
      "extensions": [
        "js",
        "json",
        "lock",
        "tsx",
        "ts"
      ]
    }
  }
}
